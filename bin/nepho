#!/usr/bin/env python
import pdb
import nepho
from nepho.command import command
from nepho.aws import clidriver 
import sys
import json

 
def main():
    
    output = command()
    args = json.loads(output)

    driver = args['opts']['driver']

    if driver == 'aws':
      
        return nepho.aws.clidriver.main(output)


if __name__ == '__main__':
    sys.exit(main())
    
print output
